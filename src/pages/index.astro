---
import Layout from "../layouts/Layout.astro";
import HeroSection from "../components/sections/HeroSection.astro";

import * as page from "../content/en.md";
import FeaturesSection from "../components/sections/FeaturesSection.astro";
import BenefitsSection from "../components/sections/BenefitsSection.astro";
import PricingSection from "../components/sections/PricingSection.astro";
import RequestDemoSection from "../components/sections/RequestDemoSection.astro";
import FaqSection from "../components/sections/FaqSection.astro";
import ArticlesSection from "../components/sections/ArticlesSection.astro";
import HowItWorksSection from "../components/sections/HowItWorksSection.astro";
---

<Layout pageContent={page}>
  <HeroSection />
  <HowItWorksSection />
  <FeaturesSection data={page.frontmatter.featuresSection} />
  <BenefitsSection data={page.frontmatter.benefitsSection} />
  <PricingSection />
  <RequestDemoSection data={page.frontmatter.requestDemoSection} />
  <FaqSection data={page.frontmatter.faqSection} />
  <ArticlesSection />
</Layout>
