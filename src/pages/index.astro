---
import Layout from "../layouts/Layout.astro";
import Heroblock from "../components/Heroblock.astro";

import * as page from "../../content/en.md";
import Features from "../components/Features.astro";
import Benefits from "../components/Benefits.astro";
import Pricing from "../components/Pricing.astro";
import RequestDemo from "../components/RequestDemo.astro";
import Faq from "../components/Faq.astro";
import FeaturedBlogPosts from "../components/FeaturedBlogPosts.astro";
---

<Layout pageContent={page}>
  <Heroblock data={page.frontmatter.heroblock} language="en" />
  <Features data={page.frontmatter.features} />
  <Benefits data={page.frontmatter.benefits} />
  <Pricing pricing={page.frontmatter.pricing} />
  <RequestDemo />
  <Faq faq={page.frontmatter.faq} />
  <FeaturedBlogPosts />
